{
    "name": "phossa/phossa-route",
    "license": "MIT",
    "type": "library",
    "description": "A fast and full-fledged routing libraray for PHP",
    "keywords": ["phossa"],
    "homepage": "https://github.com/phossa/phossa-route",
    "authors": [
        {
            "name": "Hong Zhang",
            "homepage": "https://github.com/phossa"
        }
    ],
    "autoload": {
        "psr-0": { "Phossa\\Route\\": "src/" }
    },
    "autoload-dev": {
        "psr-0": { "Phossa\\Route\\": "tests/src/" }
    },
    "require": {
        "php": ">=5.4.0",
        "phossa/phossa-shared": ">=1.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phossa/phossa-logger": "*"
    },
    "archive": {
        "exclude": ["!vendor", "tests", "*phpunit.xml",  "composer.lock"]
    }
}
